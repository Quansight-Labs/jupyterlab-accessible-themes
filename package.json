{
  "name": "jupyterlab-accessible-themes",
  "version": "0.2.0",
  "description": "A collection of accessible JupyterLab themes",
  "private": true,
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/github_username/jupyterlab-accessible-themes",
  "bugs": {
    "url": "https://github.com/github_username/jupyterlab-accessible-themes/issues"
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "Jupyter Accessibility team",
    "email": "contact@quansight.com"
  },
  "workspaces": [
    "packages/*"
  ],
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,js,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "repository": {
    "type": "git",
    "url": ".git"
  },
  "scripts": {
    "build": "jlpm build:lib && jlpm build:ext",
    "build:prod": "jlpm clean && jlpm build:lib && jlpm build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "lerna run build:lib",
    "build:ext": "lerna run build:ext",
    "dev": "yarn dev:py && yarn dev:ext",
    "dev:py": "pip install -e.",
    "dev:ext": "jupyter labextension develop --overwrite .",
    "clean": "rimraf \"./packages/*/lib\" \"packages/*/*.tsbuildinfo\" ./jupyterlab_gt_coar_theme/labextensions",
    "eslint": "jlpm eslint:check --fix",
    "eslint:check": "eslint . --cache --ext .ts,.tsx",
    "install:extension": "jlpm build",
    "lint": "jlpm stylelint && jlpm prettier && jlpm eslint",
    "lint:check": "jlpm stylelint:check && jlpm prettier:check && jlpm eslint:check",
    "prettier": "jlpm prettier:base --write --list-different",
    "prettier:base": "prettier \"**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}\"",
    "prettier:check": "jlpm prettier:base --check",
    "stylelint": "jlpm stylelint:check --fix",
    "stylelint:check": "stylelint --cache \"style/**/*.css\"",
    "test": "jest --coverage",
    "watch:ext": "lerna run watch:ext --stream --parallel",
    "watch:lib": "lerna run watch:lib --stream"
  },
  "dependencies": {
    "@jupyterlab/application": "^3.1.0",
    "@jupyterlab/apputils": "^3.1.0"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^3.1.0",
    "@jupyterlab/testutils": "^3.0.0",
    "@types/jest": "^26.0.0",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.0.0",
    "lerna": "^5.6.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "stylelint": "^14.3.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^6.0.0",
    "stylelint-config-standard": "~24.0.0",
    "stylelint-prettier": "^2.0.0",
    "ts-jest": "^26.0.0",
    "typescript": "~4.1.3"
  },
  "sideEffects": [
    "style/*.css"
  ],
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true
  }
}
